{
  "name": "JtoD",
  "displayName": "json to dart",
  "description": "convert JSON data into DART data model",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.70.0"
  },
  "repository": {
		"type": "git",
		"url": "https://github.com/praveen576232/JsonToDart"
	},
	"bugs": {
		"url": "https://github.com/praveen576232/JsonToDart/issues"
	},
	"publisher": "praveend",
	"icon": "screenshots/dart.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:JtoD.JtoD"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "JtoD.JtoD",
      "title": "JSON to Dart"
    }],
    "keybindings":[
			{
				"command": "JtoD.JtoD",
				"key": "alt+d"
			
			}
		]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
